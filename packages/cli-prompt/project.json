{
  "name": "cli-prompt",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/cli-prompt/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "outputs": ["{options.outputPath}"],
      "options": { "script": "build", "project": "packages/cli-prompt/package.json" }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": { "lintFilePatterns": ["packages/cli-prompt/**/*.ts", "packages/cli-prompt/package.json"] }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": { "jestConfig": "packages/cli-prompt/jest.config.ts", "passWithNoTests": true },
      "configurations": { "ci": { "ci": true, "codeCoverage": true } }
    }
  },
  "tags": []
}
